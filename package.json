{
  "name": "holokaiguys-account-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:test": "dotenv -e .env.test -- prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:dev:createonly": "prisma migrate dev --create-only",
    "db:reset": "prisma migrate reset --force",
    "db:reset:test": "dotenv -e .env.test -- prisma migrate reset --force",
    "script:setsuperadmin": "node scripts/set-super-admin.js",
    "start": "pnpm run db:generate && pnpm run db:migrate && nest start",
    "start:dev": "pnpm run db:generate && pnpm run db:migrate && nodemon",
    "start:debug": "pnpm run db:generate && pnpm run db:migrate && nodemon --debug",
    "start:prod": "node dist/main",
    "lint": "eslint --max-warnings=0 \"**/*.{js,ts}\" && tsc --noEmit",
    "lint:fix": "eslint --max-warnings=0 --fix \"**/*.{js,ts}\" && tsc --noEmit",
    "test:clear": "rimraf coverage && jest --clearCache",
    "test": "pnpm run db:generate && dotenv -e .env.test -- jest",
    "test:db": "pnpm run db:reset:test && dotenv -e .env.test -- jest --runInBand --config jest-db.config.js",
    "test:all": "pnpm run db:reset:test && dotenv -e .env.test -- jest --runInBand --config jest-all.config.js",
    "test:watch": "pnpm run test --watch",
    "test:watch:db": "pnpm run test:db --watch",
    "test:watch:all": "pnpm run test:all --watch",
    "test:cov": "pnpm run test --coverage",
    "test:cov:db": "pnpm run test:db --coverage",
    "test:cov:all": "pnpm run test:all --coverage",
    "precommit": "lint-staged",
    "prepare": "husky"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.7",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.7",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@prisma/client": "^5.12.1",
    "axios": "^1.6.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "ioredis": "^5.3.2",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "passport-http-bearer": "^1.0.1",
    "prisma": "^5.12.1",
    "prisma-error-enum": "^0.1.3",
    "qs": "^6.12.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.7",
    "@stylistic/eslint-plugin": "^1.7.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.6",
    "@types/passport": "^1.0.16",
    "@types/passport-discord": "^0.1.13",
    "@types/passport-http-bearer": "^1.0.41",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "lodash": "^4.17.21",
    "nodemon": "^3.1.0",
    "passport-custom": "^1.1.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.4"
  }
}
